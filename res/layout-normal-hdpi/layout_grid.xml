<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/ttt"
    android:gravity="center_horizontal"
    tools:context=".MultiplayerActivity" >

    <RelativeLayout
        android:id="@+id/row3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/row2"
        android:layout_marginTop="30dp"
        android:gravity="center" >

        <ImageView
            android:id="@+id/otile32"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile31"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile31"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="12dp"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile33"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile32"
            android:src="@drawable/blank" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/row1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="23dp"
        android:gravity="center" >

        <ImageView
            android:id="@+id/otile12"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile11"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile11"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="12dp"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile13"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile12"
            android:src="@drawable/blank" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/row2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/row1"
        android:layout_marginTop="34dp"
        android:gravity="center" >

        <ImageView
            android:id="@+id/otile22"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile21"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile21"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="12dp"
            android:src="@drawable/blank" />

        <ImageView
            android:id="@+id/otile23"
            android:layout_width="80dp"
            android:layout_height="130dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@+id/otile22"
            android:src="@drawable/blank" />
    </RelativeLayout>

</RelativeLayout>